<h1>dashboard</h1>
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
    <% @favorites.each do |favorite|%>
      <div class="card-category"  style="background-image: url('<%= cl_image_path favorite.photo, height: 300, width: 400, crop: :fill %>')">
        <div class="specialty-name"><%= link_to favorite.name, specialty_path(favorite), class: 'card-link' %>
        </div>
        <i class="far fa-heart favorites <%= current_user.favorited?(favorite) ? "liked" : "" %>" data-id="<%= favorite.id %>"></i>
      </div>
    <% end %>
  </div>
  <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">...</div>

</div>
